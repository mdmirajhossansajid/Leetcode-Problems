https://leetcode.com/problems/make-the-string-great/description/

class Solution {
public:
    string makeGood(string s) {
        stack<char>st;
        for(int i=0;i<s.length();i++)
        {
            if(st.empty())
                st.push(s[i]);
            else if(st.top()==(s[i+1]+'65'))
                st.pop();
                else if(st.top()==(s[i+1]+'65'))
                st.pop();
            else
            st.push(s[i]);
        }
        string result;
        while(!st.empty())
        {
            result+=st.top();
            st.pop();
        }
        reverse(result.begin(),result.end());
        return result;
    }
};
